;\n} \n\nfunction myAlert(text, type) {\n     refs.picsGallaryMarkUp.innerHTML='';\nalert({\n  text,\n  type,\n});};"},"lineCount":null}},"error":null,"hash":"505cfac87d3d045fa8bcd0ade9ee2f4f","cacheData":{"env":{}}}